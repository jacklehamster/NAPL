  "/example/package.json": {
    "name": "example",
    "module": "src/index.ts",
    "type": "module",
    "scripts": {
      "start": "bun run build && bun --watch index.ts",
      "start:prod": "bun run build && NODE_ENV=production bun --watch index.ts",
      "build": "bun i && bun run build-icon && bun run copy-workers && bun run build-workers && bun run bundler/bundler.ts",
      "build-icon": "mkdir -p icons && icon-gen -i ../icon.png -o ./icons",
      "dev": "bun run build && bun --watch index.ts",
      "copy-workers": "rm -rf ./dist && sleep 1 && mkdir -p dist && cp ../node_modules/@dobuki/hello-worker/dist/signal-room.worker.js* ./dist",
      "build-workers": "mkdir -p dist && bun build ./src/worker-room/app.worker.ts --outdir ./dist --target=browser --format=esm --sourcemap"
    },
    "devDependencies": {
      "@types/express": "^5.0.6",
      "bun-lightningcss": "^0.2.0",
      "bun-types": "^1.3.6",
      "dotenv": "^16.6.1",
      "express": "^4.22.1",
      "icon-gen": "^4.0.0",
      "png-to-ico": "^2.1.8",
      "serve-static-bun": "^0.5.3"
    },
    "peerDependencies": {
      "typescript": "^5.9.3"
    },
    "dependencies": {
      "napl": "link:napl",
      "@types/random-name": "^0.1.2",
      "generate-random-emoji": "^1.0.2",
      "json-stringify-pretty-compact": "^4.0.0",
      "random-name": "^0.1.2",
      "unique-names-generator": "^4.7.1"
    }
