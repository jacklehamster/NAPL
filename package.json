{
  "name": "napl",
  "version": "1.0.103",
  "module": "src/index.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/jacklehamster/NAPL.git"
  },
  "homepage": "https://jacklehamster.github.io/NAPL/",
  "peerDependencies": {
    "typescript": "latest"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "test": "bun test",
    "build": "bun i && rm -rf *.tsbuildinfo && rm -rf dist/*.* && bun run bundler/bundler.ts && bun run test && bun run build:declaration && bun link napl",
    "build:declaration": "tsc --emitDeclarationOnly --project tsconfig.types.json",
    "build:example": "cd example && bun i && bun run build && cd ..",
    "example": "cd example && bun start && cd ..",
    "example:prod": "cd example && bun start:prod && cd ..",
    "build:dev": "bun run build:example && cd example && bun run build",
    "dev": "bun run build:dev && bun --watch index.ts",
    "fix-package-name": "npm explore package-name-fixer -- bun start \"$(pwd)\"",
    "npm-patch": "npm version patch && npm publish"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "author": {
    "name": "Jack Le Hamster",
    "email": "jacklehamster@gmail.com"
  },
  "description": "Not Another Programming Language",
  "devDependencies": {
    "@dobuki/dev-dependencies": "^1.0.20",
    "@types/jest": "^29.5.14",
    "@types/ws": "^8.18.1",
    "bun": "^1.3.6",
    "fetch-blob": "^4.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.6"
  },
  "dependencies": {
    "@dobuki/data-blob": "^1.0.18",
    "@dobuki/hello-worker": "^1.0.58",
    "@msgpack/msgpack": "^3.1.3",
    "@types/npm": "^7.19.3"
  }
}
